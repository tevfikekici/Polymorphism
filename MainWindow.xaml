<Window x:Class="Polymorphism.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Polymorphism"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="button" Content="Message" HorizontalAlignment="Left" Margin="25,32,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button_P" Content="Polymorphism Message" HorizontalAlignment="Left" Margin="105,32,0,0" VerticalAlignment="Top" Width="144" Click="button_P_Click"/>

    </Grid>
</Window>
